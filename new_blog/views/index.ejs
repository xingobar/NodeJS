<%- include header %>

<% posts.forEach(function(post,index){ %>
<p>
  <h2>
    <a href="#">
      <%= post.title %>
    </a>
  </h2>
</p>
<p class="info">
  作者: <a href="#"><%= post.name %></a>
  日期 :<a href="#"><%= post.time.minute %></a>
</p>
<p><%= post.post %></p>
<% }) %>

<%- include footer %>
